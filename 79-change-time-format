public String changeTimeFormat(String strTime) {
    String militaryTime;
    if (strTime.contains('p') || strTime.contains('P')) {
        strTime = strTime.replace(' PM', '');
        militaryTime = Math.mod(strTime, 12) + 12 + strTime.substring(2);
    } else {
        // string methods to remove : and then space AM
    }
    return militaryTime;
}