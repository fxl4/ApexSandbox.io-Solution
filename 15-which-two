public String whichTwo(Integer a, Integer b, Integer c) {
    //code here
    Integer num1 = a + b;
    Integer num2 = a + c;
    Integer num3 = b + c;
    if (num1 == c) {
        return String 'c';
    } else if (num2 == b) {
        return String 'b';
    } else if (num3 == a) {
        return String 'a';
    }
}