public Map<String, Account> phoneToAccount(List<Account> accounts) {
    //code here
    Map<String, Account> m1 = new Map<String, Account>();
    List<String> phoneList = new List<String>();
    for (Account acc : accounts){
        if (acc.Phone != null){
            m1.put(acc.Phone, acc);
        }
    }
    return m1;
}