public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    Integer currentIndex = 0;
    String currentName = '';
    List<String> completeNames = new List<String>();

    if (firstNames.size() == lastNames.size()){
        while (currentIndex < firstNames.size()){
            currentName = firstNames[currentIndex] + ' ' + lastNames[currentIndex];
            completeNames.add(currentName);
            currentIndex += 1;
        }
    }
    
    return completeNames;
}