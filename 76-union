public Set<Integer> setUnion(Set<Integer> set1, Set<Integer> set2) {
    //code here
    Set<Integer> set3 = new Set<Integer>();
    for (Integer i : set1){
        if (!set3.contains(i)){
            set3.add(i);
        }
    }
    for (Integer j : set2){
        if (!set3.contains(j)){
            set3.add(j);
        }
    }
    return set3;
}