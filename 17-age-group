public String ageGroup(Integer n) {

    String group = '';
    if (n >= 0 && n <= 1) {
        group = 'Infant';
        return group;
    } else if (n >= 2 && n <= 14) {
        group = 'Child';
        return group;
    } else if (n >= 15 && n <= 21) {
        group = 'Youth';
        return group;
    } else if (n >= 22) {
        group = 'Adult';
        return group;
    } 

}