public String ageGroup(Integer n) {

    if (n >= 0 && n <= 1) {
        return String  'Infant';
    } else if (n >= 2 && n <= 14) {
        return String  'Child';
    } else if (n >= 15 && n <= 21) {
        return String  'Youth';
    } else if (n >= 22) {
        return String 'Adult';
    } 

}