public String ageGroup(Integer n) { 
    String resultingGroup = '';
    if (n >= 0 && n <= 1) {
        resultingGroup = 'Infant';
    } else if (n >= 2 && n <= 14) {
        resultingGroup = 'Child';
    } else if (n >= 15 && n <= 21) {
        resultingGroup = 'Youth';
    } else if (n >= 22) {
        resultingGroup = 'Adult';
    } 
    return resultingGroup;
}